<!--pages/AR/pages/threeD/three.wxml-->
<view
  class="basicview"
  bindtouchstart="touchStart"
  bindtouchmove="touchMove"
  bindtouchend="touchEnd"
  bindtouchcancel="touchCancel"
  bindtap="touchTap"
  style="height:{{screenHeight}}px;width:{{screenWidth}}px;">
    <!--摄像头层-->
    <camera 
    class="basiccamera"
    mode="normal" 
    device-position="back" 
    flash="auto" 
    frame-size="medium"
    style="position:fixed;top:0;width:100%;height:100%;z-index:0;">
    <canvas 
  class="webgl"
  type="webgl"
  id="webgl"
  style="height:100%;width:100%;z-index:100%"
  >
  <view class="paizhao" catchtap ="paizhaoClick">
    拍照
  </view>
  <view class="return" catchtap ="returnClick">
    返回
  </view>
  <view class="info" visibility = "hidden">
    {{info}}
  </view>
  <view class="countTime">
        <text>计时：{{countdown}}</text>
    </view>
 </canvas>
  </camera>
</view>