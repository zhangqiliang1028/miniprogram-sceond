<!--pages/AR/pages/map/map.wxml-->
<map id="map" 
      longitude="{{longitude}}"
      latitude="{{latitude}}"
      scale="{{scale}}"
      controls="{{controls}}"
      markers="{{markers}}"   
      polyline="{{polyline}}" 
      circles="{{circles}}" 
      setting = "{{setting}}"
      enable-poi = "true"
      enable-building = "true"
      bindmarkertap = "markertap"
      bindcontroltap = "controltap"
      bindregionchange = "regionchange"
      bindanchorpointtap = "anchorpointtap"
      style="width: 100%; height:{{screenHeight}}px;">
      <cover-view>
        <cover-view slot="callout">
          <cover-view marker-id="1"></cover-view>
          <cover-view marker-id="2"></cover-view>
        </cover-view>
      </cover-view>

</map>

